# LlamaIndex Backend Environment Example
# Copy this file to .env and fill in the values for local development

# ======== SUPABASE CONFIGURATION ========
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=eyJhbGc...your-anon-key-here
SUPABASE_JWT_SECRET=your-jwt-secret-here
SUPABASE_SERVICE_ROLE_KEY=eyJhbGc...your-service-role-key-here

# ======== PINECONE CONFIGURATION ========
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=developer-quickstart-py

# ======== OPENAI CONFIGURATION ========
OPENAI_API_KEY=sk-your-openai-api-key

# ======== AUTHENTICATION ========
# Create a strong random string for API access
LLAMAINDEX_API_KEY=your-strong-random-api-key

# ======== DEPLOYMENT SETTINGS ========
# Must be 8000 for Railway deployment
PORT=8000

# ======== EMBEDDING CONFIGURATION ========
EMBEDDING_MODEL=text-embedding-3-large
EMBEDDING_DIMENSIONS=3072

# ======== ENVIRONMENT SETTINGS ========
ENVIRONMENT=development
DEBUG=true

# ======== FRONTEND INTEGRATION ========
# For CORS configuration
FRONTEND_URL=http://localhost:3000
